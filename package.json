  },
  "homepage": "https://github.com/stevenvelozo/fable",
  "devDependencies": {
    "chai": "3.4.1",
    "codeclimate-test-reporter": "0.1.1",
    "coveralls": "2.11.4",
    "groc": "^0.8.0",
    "istanbul": "0.4.1",
    "mocha": "2.3.4"
  },
  "dependencies": {
    "fable-log": "~1.0.1",
